# Keybindings
unbind-key J
unbind-key K
unbind-key h
unbind-key l
unbind-key j
unbind-key k
unbind-key UP
unbind-key DOWN
unbind-key ENTER
unbind-key o


bind-key j down
bind-key k up
bind-key J next
bind-key K prev
bind-key h next-feed
bind-key l prev-feed
bind-key ENTER open-in-browser
bind-key o open


# Options
#browser "w3m %u"
bookmark-cmd "~/.newsbeuter/bookmark.sh"
auto-reload yes
reload-time 10
