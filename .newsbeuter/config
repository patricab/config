# Keybindings
unbind-key J
unbind-key K
unbind-key h
unbind-key l
unbind-key j
unbind-key k
unbind-key UP
unbind-key DOWN
unbind-key ENTER
unbind-key o
unbind-key O

# Keybindings
bind-key j down
bind-key k up
bind-key J next
bind-key K prev
bind-key h next-feed
bind-key l prev-feed
bind-key ENTER open-in-browser-and-mark-read
bind-key o open
bind-key O open-in-browser
bind-key H home
bind-key L end
bind-key m toggle-article-read


#macro i set browser "w3m %u"; open-in-browser; set browser "lynx %u"

# Options
#browser "w3m %u"
bookmark-cmd "~/.newsbeuter/bookmark.sh"
bookmark-autopilot yes
bookmark-interactive no
auto-reload yes
reload-time 10
